{
    "name": "rainbow-cli",
    "version": "1.68.0-beta.0",
    "description": "Rainbow CLI application based on the Rainbow SDK for Node.js",
    "main": "index.js",
    "scripts": {
        "test:sandbox": "sh afterbuild_sandbox.sh",
        "test": "sh afterbuild_preprod.sh",
